{
  "manifest_version": 3,
  "version": "1.2.0",
  "action": {},
  "commands": {
    "switchIncognitoMode": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Switch to incognito/normal (opens in the most recently used window of that mode)"
    }
  },
  "permissions": ["tabs", "contextMenus", "notifications"],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "default_locale": "en",
  "name": "Incognito Switcher",
  "description": "__MSG_extDesc__",
  "background": {
    "service_worker": "js/background.js"
  }
}
