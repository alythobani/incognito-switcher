{
  "manifest_version": 2,
  "name": "Incognito or not",
  "description": "Quickly switch current tab or link from incognito window to normal window, or vice versa.",
  "version": "1.1.2",
  "browser_action": {},
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
