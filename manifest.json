{
  "manifest_version": 2,
  "name": "show-up",
  "description": "Quickly change current tab or open link from incognito tab to normal tab.",
  "version": "1.1",
  "browser_action": {},
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
